{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "spring": {
        "application": {
            "name": "service-a"
        }

    },
    "eureka": {
      "client": {
        "serviceUrl": "http://localhost:8761/eureka/",
        "shouldFetchRegistry": true,
        "validateCertificates": false
      },
      "instance": {
        "port": 9001,
        "instanceId": "localhost:9001",
        "hostName": "localhost",
        "healthCheckUrlPath": "/api/values/healthcheck",
        "statusPageUrlPath": "/api/values/info"
      }
    }
}
